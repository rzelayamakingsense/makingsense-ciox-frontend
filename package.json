{
  "name": "makingsense-ciox-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "start:mock": "ng serve -c mock",
    "build:mock": "ng build -c mock",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:all": "jest --watchAll --coverage",
    "lint": "ng lint && tsc --noEmit && eslint . --ext js,ts,json --quiet --fix",
    "lint:ts": "eslint --color -c .eslintrc.json --ext .ts ."
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@okta/okta-angular": "^4.1.0",
    "@okta/okta-auth-js": "^5.5.0",
    "@okta/okta-signin-widget": "^5.11.1",
    "@popperjs/core": "^2.10.1",
    "bootstrap": "^5.1.1",
    "ngx-bootstrap": "^7.1.0",
    "ngx-bootstrap-icons": "^1.6.0",
    "ngx-spinner": "^12.0.0",
    "ngx-toastr": "^14.1.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "~12.2.7",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "^7.26.0",
    "jest": "^27.2.3",
    "jest-preset-angular": "^10.0.1",
    "prettier": "^2.4.1",
    "typescript": "~4.3.5"
  }
}
